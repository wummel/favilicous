{
  "applications": {
    "gecko": {
      "id": "reflexionsniveau@web.de",
      "strict_min_version": "57.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "object-src 'none'; script-src 'self'; img-src 'self' https://favicons.githubusercontent.com",
  "default_locale": "en",
  "description": "Displays bookmarks in the new-tab page.",
  "homepage_url": "https://github.com/wummel/favilicous/",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "Favilicous",
  "permissions": [
    "bookmarks"
  ],
  "version": "0.21",
  "web_accessible_resources": [
    "newtab.html"
  ]
}